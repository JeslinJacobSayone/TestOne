<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sayone.testone">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".app.TestApplicationClass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".app.login.registration.FirstPageActivity">


        </activity>
        <activity
            android:name=".app.login.registration.DialogDate"
            android:theme="@style/customdate" />
        <activity android:name=".app.settings.ProfileViewActivity" />
        <activity android:name=".app.homepage.HomePageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".app.homepage.WebActivity" />
        <activity android:name=".app.settings.SettingsActivity"></activity>
    </application>

</manifest>